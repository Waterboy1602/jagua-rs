    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceRoot}/lbf/target/debug/lbf.exe",
            "args": [
                // "-i", "${workspaceRoot}/assets/dxf/albano_dxf.json",
                "-i", "${workspaceRoot}/assets/albano.json",
                "-c", "${workspaceRoot}/assets/config_lbf.json",
                "-s", "${workspaceRoot}/solutions",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}",
            "environment": [],
            "console": "integratedTerminal",
            "preLaunchTask": "cargo build"
        },
    ]
}